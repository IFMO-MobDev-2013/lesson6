<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ru.ifmo.rssreader"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="12"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="InputActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="ShowTitlesActivity"
                  android:label="@string/app_name">
        </activity>
        <activity android:name="ShowArticleActivity"
                  android:label="@string/app_name">
        </activity>
       <service android:enabled="true" android:name=".MyIntentService">
           <intent-filter>
               <action android:name="ru.ifmo.rssreader.MyIntentService"/>
           </intent-filter>
       </service>
    </application>
</manifest>
